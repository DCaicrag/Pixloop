<launch>

  <arg name="launch_driver" default="true" />

  <group ns="gnss">

    <!-- Ublox Driver -->
    <!-- <remap from="ublox/fix" to="ublox/nav_sat_fix" />
    <include file="$(find ublox_gps)/launch/ublox_device.launch" if="$(eval launch_driver==true)">
      <arg name="node_name" value="ublox" />
      <arg name="param_file_name" value="c94_f9p_rover" />
    </include> -->

    <include file = "$(find nmea_navsat_driver)/launch/nmea_serial_driver.launch">
      <arg name="port" value="/dev/ttyS0" />
      <arg name="baud" value="115200" />
    </include>

    <!-- NavSatFix to MGRS Pose -->
    <include file="$(find gnss_poser)/launch/gnss_poser.launch">
      <arg name="input_topic_fix" value="/sensing/gnss/fix" />
      <arg name="input_topic_navpvt" value="/sensing/gnss/ublox/navpvt" />

      <arg name="output_topic_gnss_pose" value="pose" />
      <arg name="output_topic_gnss_pose_cov" value="pose_with_covariance" />
      <arg name="output_topic_gnss_fixed" value="fixed" />

      <arg name="coordinate_system" value="0" doc="0:UTM, 1:MGRS, 2:PLANE" />
      <arg name="use_ublox_receiver" value="true" />

      <arg name="gnss_frame" value="gnss_link" />
    </include>

  </group>
</launch>
